{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "globals": {
        "logger": "readonly",
        "describe": "readonly",
        "it": "readonly"
    },
    "rules": {
        "comma-dangle": 0,
        "no-console": "error",
        "max-lines-per-function": ["error", {"max": 30, "skipBlankLines": true}],
        "complexity": ["error", 5],
        "consistent-return": 0,
        "camelcase": 0,
        "no-param-reassign": 0,
        "no-shadow": [
          "error",
          {
            "allow": ["req", "res", "err", "next"]
          }
        ],
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "arrow-parens": 0,
        "max-lines": ["error", {"max": 315, "skipBlankLines": true}],
        "max-depth": ["error", 3]
    }
}
